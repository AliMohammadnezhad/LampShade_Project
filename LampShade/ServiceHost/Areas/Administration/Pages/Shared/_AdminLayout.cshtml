<!DOCTYPE html>
<html lang="en" dir="rtl">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
    <meta name="author" content="Coderthemes">

    <link rel="shortcut icon" href="~/AdminTheme/assets/images/favicon.ico">

    <title>آقای ادمین | قالب واکنشگرا مدیریتی</title>

    <!--Morris Chart CSS -->
    <link rel="stylesheet" href="~/AdminTheme/assets/plugins/morris/morris.css">
    <link href="~/AdminTheme/assets/css/persian-datepicker.min.css" rel="stylesheet" />
    <!-- App css -->
    <link href="~/AdminTheme/assets/css/bootstrap-rtl.min.css" rel="stylesheet" type="text/css"/>
    <link href="~/AdminTheme/assets/css/core.css" rel="stylesheet" type="text/css"/>
    <link href="~/AdminTheme/assets/css/components.css" rel="stylesheet" type="text/css"/>
    <link href="~/AdminTheme/assets/css/icons.css" rel="stylesheet" type="text/css"/>
    <link href="~/AdminTheme/assets/css/pages.css" rel="stylesheet" type="text/css"/>
    <link href="~/AdminTheme/assets/css/menu.css" rel="stylesheet" type="text/css"/>
    <link href="~/AdminTheme/assets/css/responsive.css" rel="stylesheet" type="text/css"/>
    <link href="~/AdminTheme/assets/plugins/datatables/jquery.dataTables.min.css" rel="stylesheet" type="text/css"/>
    <link href="~/AdminTheme/assets/plugins/toastr/toastr.css" rel="stylesheet"/>
    <!-- HTML5 Shiv and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->

    <script src="~/AdminTheme/assets/js/modernizr.min.js"></script>

</head>


<body class="fixed-left">

<!-- Begin page -->
<div id="wrapper">

    <!-- Top Bar Start -->
    <div class="topbar">

        <!-- LOGO -->
        <div class="topbar-left">
            <a href="index.html" class="logo">
                <span>آقای<span>ادمین</span></span><i class="zmdi zmdi-layers"></i>
            </a>
        </div>

        <!-- Button mobile view to collapse sidebar menu -->
        <div class="navbar navbar-default" role="navigation">
            <div class="container">

                <!-- Page title -->
                <ul class="nav navbar-nav navbar-left">
                    <li>
                        <button class="button-menu-mobile open-left">
                            <i class="zmdi zmdi-menu"></i>
                        </button>
                    </li>
                    <li>
                        <h4 class="page-title">داشبورد</h4>
                    </li>
                </ul>

                <!-- Right(Notification and Searchbox -->
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <!-- Notification -->
                        <div class="notification-box">
                            <ul class="list-inline m-b-0">
                                <li>
                                    <a href="javascript:void(0);" class="right-bar-toggle">
                                        <i class="zmdi zmdi-notifications-none"></i>
                                    </a>
                                    <div class="noti-dot">
                                        <span class="dot"></span>
                                        <span class="pulse"></span>
                                    </div>
                                </li>
                            </ul>
                        </div>
                        <!-- End Notification bar -->
                    </li>
                    <li class="hidden-xs">
                        <form role="search" class="app-search">
                            <input type="text" placeholder="به دنبال چه می گردی ؟؟؟"
                                   class="form-control">
                            <a href="">
                                <i class="fa fa-search"></i>
                            </a>
                        </form>
                    </li>
                </ul>

            </div><!-- end container -->
        </div><!-- end navbar -->
    </div>
    <partial name="_Menu"/>
    <div class="content-page">
        <!-- Start content -->
        <div class="content">
            <div class="container">


                @RenderBody()

            </div> <!-- container -->

        </div> <!-- content -->

        <footer class="footer">
            آقای ادمین2016 ©.
        </footer>

    </div>


</div>
<div class="control-group" id="toastTypeGroup">
    <div class="controls">
        <label>نوع پیغام</label>
        <div class="radio radio-success">
            <input type="radio" name="radio" id="radio1" value="success">
            <label for="radio1">
                قبول
            </label>
        </div>

        <div class="radio radio-info">
            <input type="radio" name="radio" id="radio2" value="info">
            <label for="radio2">
                اعلان
            </label>
        </div>

        <div class="radio radio-warning">
            <input type="radio" name="radio" id="radio3" value="warning">
            <label for="radio3">
                توجه
            </label>
        </div>

        <div class="radio radio-danger">
            <input type="radio" name="radio" id="radio4" value="error">
            <label for="radio4">
                خظا
            </label>
        </div>

    </div>
</div>
<div id="MainModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content" id="ModalContent">


        </div>
    </div>
</div>

<script>
    var resizefunc = [];
</script>

<!-- jQuery  -->
<script src="~/AdminTheme/assets/js/jquery.min.js"></script>
<script src="~/AdminTheme/assets/js/bootstrap-rtl.min.js"></script>
<script src="~/AdminTheme/assets/js/detect.js"></script>
<script src="~/AdminTheme/assets/js/fastclick.js"></script>
<script src="~/AdminTheme/assets/js/jquery.blockUI.js"></script>
<script src="~/AdminTheme/assets/js/waves.js"></script>
<script src="~/AdminTheme/assets/js/jquery.nicescroll.js"></script>
<script src="~/AdminTheme/assets/js/jquery.slimscroll.js"></script>
<script src="~/AdminTheme/assets/js/jquery.scrollTo.min.js"></script>

<!-- KNOB JS -->
<!--[if IE]>
    <script type="text/javascript" src="~/AdminTheme/assets/plugins/jquery-knob/excanvas.js"></script>
<![endif]-->
<script src="~/AdminTheme/assets/plugins/jquery-knob/jquery.knob.js"></script>

<!--Morris Chart-->
<script src="~/AdminTheme/assets/plugins/morris/morris.min.js"></script>
<script src="~/AdminTheme/assets/plugins/raphael/raphael-min.js"></script>

<!-- Dashboard init -->
<script src="~/AdminTheme/assets/pages/jquery.dashboard.js"></script>

<!-- App js -->
<script src="~/AdminTheme/assets/js/jquery.validate.min.js"></script>
<script src="~/AdminTheme/assets/js/jquery.validate.unobtrusive.min.js"></script>
<script src="~/AdminTheme/assets/js/jquery.core.js"></script>
<script src="~/AdminTheme/assets/js/jquery.app.js"></script>
<script src="~/AdminTheme/assets/plugins/toastr/toastr.min.js"></script>
<script src="~/AdminTheme/assets/js/site.js"></script>
@RenderSection("Scripts", false)
<script src="~/AdminTheme/assets/js/persian-date.min.js"></script>
<script src="~/AdminTheme/assets/js/persian-datepicker.min.js"></script>
<script type="text/javascript">
    $('.persianDateInput').persianDatepicker({
        format: 'YYYY/MM/DD',
        autoClose: true,
        initialValue: false
    });
</script>
</body>
</html>